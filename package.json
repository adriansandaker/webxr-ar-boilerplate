{
  "name": "webxr-ar-boilerplate",
  "version": "1.0.0",
  "description": "AR with WebXR and BabylonJS.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --mode development --config webpack.config.js",
    "start:live": "concurrently \"yarn start\" \"node ./ngrok/ngrok-helper.js\"",
    "build": "webpack -p",
    "esbuild": "esbuild --bundle ./src/index.js --outfile=./dist/bundle.js --minify --sourcemap",
    "stats": "webpack --profile --json > stats.json",
    "bundle-analyzer": "webpack-bundle-analyzer stats.json",
    "analyze": "npm run stats && npm run bundle-analyzer"
  },
  "author": "adriansandaker",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babylonjs/inspector": "^4.1.0",
    "@types/uuid": "^8.3.0",
    "@types/webpack": "^4.41.22",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "ngrok": "^3.3.0",
    "script-loader": "^0.7.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^8.0.4",
    "tslint": "^6.1.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@babylonjs/core": "^4.2.0-beta.9",
    "@babylonjs/gui": "^4.2.0-beta.9",
    "@babylonjs/loaders": "^4.2.0-beta.9",
    "uuid": "^8.2.0"
  }
}